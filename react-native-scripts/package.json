{
  "name": "react-native-scripts-ts",
  "version": "1.15.0",
  "description": "Configuration and scripts for Create React Native App.",
  "license": "BSD-3-Clause",
  "keywords": [
    "react-native",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mathieudutour/create-react-native-app-typescript.git",
    "directory": "react-native-scripts"
  },
  "homepage": "https://github.com/mathieudutour/create-react-native-app-typescript",
  "bugs": "https://github.com/mathieudutour/create-react-native-app-typescript",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "build",
    "template",
    "template-with-web"
  ],
  "bin": {
    "react-native-scripts-ts": "./build/bin/react-native-scripts.js"
  },
  "scripts": {
    "start": "taskr",
    "build": "taskr build",
    "prepare": "taskr build"
  },
  "dependencies": {
    "@expo/bunyan": "1.8.10",
    "babel-runtime": "^6.9.2",
    "chalk": "^2.0.1",
    "cross-spawn": "^5.0.1",
    "fs-extra": "^3.0.1",
    "indent-string": "^3.0.0",
    "inquirer": "^3.0.1",
    "lodash": "^4.17.4",
    "match-require": "^2.0.0",
    "minimist": "^1.2.0",
    "path-exists": "^3.0.0",
    "progress": "^2.0.0",
    "qrcode-terminal": "^0.11.0",
    "rimraf": "^2.6.1",
    "xdl": "48.1.4"
  },
  "devDependencies": {
    "@taskr/babel": "^1.0.6",
    "@taskr/clear": "^1.0.6",
    "@taskr/esnext": "^1.0.0",
    "@taskr/shell": "^1.0.6",
    "@taskr/watch": "^1.0.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "taskr": "^1.0.6"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation))"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "android.ts",
      "android.tsx"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-expo"
    ],
    "env": {
      "development": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      }
    }
  }
}
